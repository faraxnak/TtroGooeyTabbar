FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/EasyPeasy" "$PODS_CONFIGURATION_BUILD_DIR/PayWandBasicElements" "$PODS_CONFIGURATION_BUILD_DIR/PureLayout" "$PODS_CONFIGURATION_BUILD_DIR/SwiftyButton" "$PODS_CONFIGURATION_BUILD_DIR/UIColor_Hex_Swift"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/EasyPeasy/EasyPeasy.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/PayWandBasicElements/PayWandBasicElements.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/PureLayout/PureLayout.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftyButton/SwiftyButton.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIColor_Hex_Swift/UIColor_Hex_Swift.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "EasyPeasy" -framework "PayWandBasicElements" -framework "PureLayout" -framework "SwiftyButton" -framework "UIColor_Hex_Swift"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
